{
  "name": "purescript-with-gulp-demo",
  "version": "0.1.0",
  "description": "PureScript with Gulp demo",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "LicenseRef-LICENSE",
  "paths": {
    "dist": "dist",
    "html": "src/html/*.html",
    "lessMain": "src/less/main.less",
    "lessAll": "src/less/*.less",
    "static": "src/static/*",
    "purescript": {
      "modules": "purescript_modules",
      "sources": [
        "src/purs/**/*.purs",
        "bower_components/purescript-*/src/**/*.purs"
      ],
      "foreigns": [
        "src/purs/**/*.js",
        "bower_components/purescript-*/src/**/*.js"
      ]
    }
  },
  "author": "Carl Baatz <carl.baatz@gmail.com>",
  "repository": "https://github.com/cbaatz/prospect",
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-plugin-transform-class-properties": "^6.0.14",
    "babel-plugin-transform-flow-strip-types": "^6.0.14",
    "babel-plugin-transform-object-rest-spread": "^6.0.14",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify": "^10.2.1",
    "del": "^1.2.0",
    "errorhandler": "^1.4.2",
    "gulp": "^3.8.11",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-postcss": "^6.0.1",
    "gulp-purescript": "^0.5.0",
    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "node-notifier": "^4.2.3",
    "purescript": "^0.7.5",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.1"
  },
  "dependencies": {}
}
